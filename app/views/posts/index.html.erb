<h3 class="ui center aligned header">Stackable Grid</h3>
<div class="ui two column stackable grid">

@users

  <div class="three column row">
    <% @posts.each do |post| %>
      <div class="column">
        <div class="ui segment">
          <%= post.content %><br />

          <%= link_to "here is the link", user_post_path(:user_id, :id) %>
        </div>
      </div>
    <% end %>
  </div>

